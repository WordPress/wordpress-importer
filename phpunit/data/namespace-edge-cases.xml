<?xml version="1.0" encoding="UTF-8" ?>
<!-- Test file for tricky XML namespace scenarios -->
<rss version="2.0"
  xmlns:excerpt="http://wordpress.org/export/1.2/excerpt/"
  xmlns:content="http://purl.org/rss/1.0/modules/content/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:wp="http://wordpress.org/export/1.2/"
  xmlns:custom="http://example.com/custom/"
  xmlns:wp2="http://wordpress.org/export/1.1/"
  xmlns="">
  
<channel>
<title>Namespace Edge Cases</title>
<wp:wxr_version>1.2</wp:wxr_version>

<!-- Default namespace redefinition -->
<wp:author xmlns:wp="http://example.com/fake/">
<wp:author_id>1</wp:author_id>
<wp:author_login>fake_ns_user</wp:author_login>
<wp:author_email>fake@example.com</wp:author_email>
</wp:author>

<!-- Correct namespace after redefinition -->
<wp:author xmlns:wp="http://wordpress.org/export/1.2/">
<wp:author_id>2</wp:author_id>
<wp:author_login>real_user</wp:author_login>
<wp:author_email>real@example.com</wp:author_email>
</wp:author>

<!-- Namespace with version conflict -->
<wp2:author>
<wp2:author_id>3</wp2:author_id>
<wp2:author_login>version_conflict</wp2:author_login>
<wp2:author_email>conflict@example.com</wp2:author_email>
</wp2:author>

<!-- Custom namespace interference -->
<custom:author>
<custom:author_id>999</custom:author_id>
<custom:author_login>custom_user</custom:author_login>
<custom:author_email>custom@example.com</custom:author_email>
</custom:author>

<!-- Mixed namespace elements -->
<item xmlns:alt="http://wordpress.org/export/1.2/">
<title>Mixed Namespaces</title>
<wp:post_id>1</wp:post_id>
<alt:post_id>999</alt:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Content with namespace conflicts]]></content:encoded>

<!-- Nested namespace redefinition in postmeta -->
<wp:postmeta xmlns:wp="http://example.com/malicious/">
<wp:meta_key>malicious_key</wp:meta_key>
<wp:meta_value><![CDATA[malicious_value]]></wp:meta_value>
</wp:postmeta>

<wp:postmeta>
<wp:meta_key>normal_key</wp:meta_key>
<wp:meta_value><![CDATA[normal_value]]></wp:meta_value>
</wp:postmeta>
</item>

<!-- No namespace prefix -->
<item>
<title>No Prefix</title>
<post_id xmlns="http://wordpress.org/export/1.2/">2</post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Testing default namespace]]></content:encoded>
</item>

<!-- Empty namespace -->
<item xmlns="">
<title>Empty Namespace</title>
<wp:post_id>3</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Empty namespace test]]></content:encoded>
</item>

<!-- Unicode in namespace URI -->
<item xmlns:unicode="http://ëxämplë.com/ünïcödë/">
<title>Unicode Namespace</title>
<wp:post_id>4</wp:post_id>
<unicode:custom_field>test</unicode:custom_field>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Unicode namespace URI test]]></content:encoded>
</item>

<!-- Namespace hijacking attempt -->
<item xmlns:wp="http://malicious.example.com/hijack/">
<title>Hijacked Namespace</title>
<wp:post_id>5</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[This attempts to hijack the wp namespace]]></content:encoded>
<wp:postmeta>
<wp:meta_key>_malicious</wp:meta_key>
<wp:meta_value><![CDATA[hijacked_data]]></wp:meta_value>
</wp:postmeta>
</item>

<!-- Billion laughs namespace variant -->
<item xmlns:lol="http://example.com/lol/">
<title>Namespace Bomb</title>
<wp:post_id>6</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[&lol1;&lol1;&lol1;&lol1;&lol1;&lol1;&lol1;&lol1;&lol1;&lol1;]]></content:encoded>
</item>

<!-- Related wp namespace versions in same document -->
<wp:term xmlns:wp="http://wordpress.org/export/1.0/">
<wp:term_id>100</wp:term_id>
<wp:term_taxonomy>category</wp:term_taxonomy>
<wp:term_slug>old-version</wp:term_slug>
<wp:term_name>Old Version</wp:term_name>
</wp:term>

<!-- Conflicting wp namespace versions in same document -->
<wp:term xmlns:wp="http://different-ns.com/">
<wp:term_id>101</wp:term_id>
<wp:term_taxonomy>category</wp:term_taxonomy>
<wp:term_slug>old-version</wp:term_slug>
<wp:term_name>Old Version</wp:term_name>
</wp:term>

<wp:term>
<wp:term_id>102</wp:term_id>
<wp:term_taxonomy>category</wp:term_taxonomy>
<wp:term_slug>new-version</wp:term_slug>
<wp:term_name>New Version</wp:term_name>
</wp:term>

</channel>
</rss>
