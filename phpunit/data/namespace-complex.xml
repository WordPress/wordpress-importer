<?xml version="1.0" encoding="UTF-8" ?>
<!-- Test file for complex but legitimate namespace scenarios -->
<rss version="2.0"
  xmlns:excerpt="http://wordpress.org/export/1.2/excerpt/"
  xmlns:content="http://purl.org/rss/1.0/modules/content/"
  xmlns:dc="http://purl.org/dc/elements/1.1/"
  xmlns:wp="http://wordpress.org/export/1.2/"
  xmlns:wfw="http://wellformedweb.org/CommentAPI/"
  xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
  xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
  xmlns:georss="http://www.georss.org/georss"
  xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#">
  
<channel>
<title>Complex Namespace Scenarios</title>
<wp:wxr_version>1.2</wp:wxr_version>

<!-- Test 1: Multiple namespace versions coexisting -->
<wp:author xmlns:wp11="http://wordpress.org/export/1.1/">
<wp:author_id>1</wp:author_id>
<wp:author_login>multi_version_user</wp:author_login>
<wp:author_email>multi@example.com</wp:author_email>
<wp11:author_legacy_field>legacy_data</wp11:author_legacy_field>
</wp:author>

<!-- Test 2: Geo-tagged content with namespaces -->
<item>
<title>Geo-tagged Post</title>
<wp:post_id>1</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Post with geographical data]]></content:encoded>
<georss:point>45.256 -71.92</georss:point>
<geo:lat>45.256</geo:lat>
<geo:long>-71.92</geo:long>
</item>

<!-- Test 3: Syndication namespaces -->
<item>
<title>Syndicated Content</title>
<wp:post_id>2</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Syndicated content with metadata]]></content:encoded>
<sy:updatePeriod>hourly</sy:updatePeriod>
<sy:updateFrequency>1</sy:updateFrequency>
<slash:comments>15</slash:comments>
</item>

<!-- Test 4: Dublin Core extended metadata -->
<item>
<title>Extended Metadata</title>
<wp:post_id>3</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Post with Dublin Core metadata]]></content:encoded>
<dc:creator>John Doe</dc:creator>
<dc:subject>Technology</dc:subject>
<dc:coverage>Global</dc:coverage>
<dc:rights>Copyright 2024</dc:rights>
<dc:format>text/html</dc:format>
</item>

<!-- Test 5: Custom plugin namespaces -->
<item xmlns:plugin="http://example.com/wp-plugin/v1/">
<title>Plugin Extended</title>
<wp:post_id>4</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Post with custom plugin data]]></content:encoded>
<wp:postmeta>
<wp:meta_key>_plugin_data</wp:meta_key>
<wp:meta_value><![CDATA[a:3:{s:6:"rating";i:5;s:8:"category";s:4:"tech";s:4:"tags";a:2:{i:0;s:7:"awesome";i:1;s:4:"cool";}}]]></wp:meta_value>
</wp:postmeta>
<plugin:custom_field>
<plugin:rating>5</plugin:rating>
<plugin:category>tech</plugin:category>
<plugin:featured>true</plugin:featured>
</plugin:custom_field>
</item>

<!-- Test 6: Namespace scoping in terms -->
<wp:term xmlns:taxo="http://example.com/taxonomy/">
<wp:term_id>200</wp:term_id>
<wp:term_taxonomy>custom_tax</wp:term_taxonomy>
<wp:term_slug>scoped-term</wp:term_slug>
<wp:term_name>Scoped Term</wp:term_name>
<wp:term_parent></wp:term_parent>
<wp:termmeta>
<wp:meta_key>_taxonomy_meta</wp:meta_key>
<wp:meta_value><![CDATA[custom_value]]></wp:meta_value>
</wp:termmeta>
<taxo:custom_term_data>
<taxo:hierarchy_level>3</taxo:hierarchy_level>
<taxo:display_order>15</taxo:display_order>
</taxo:custom_term_data>
</wp:term>

<!-- Test 7: Comments with namespaces -->
<item>
<title>Post with Namespaced Comments</title>
<wp:post_id>5</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Post with complex comment data]]></content:encoded>
<wp:comment xmlns:comment="http://example.com/comment/">
<wp:comment_id>1</wp:comment_id>
<wp:comment_author><![CDATA[Commenter]]></wp:comment_author>
<wp:comment_author_email><![CDATA[comment@example.com]]></wp:comment_author_email>
<wp:comment_content><![CDATA[Great post!]]></wp:comment_content>
<wp:comment_approved><![CDATA[1]]></wp:comment_approved>
<wp:comment_type><![CDATA[]]></wp:comment_type>
<wp:comment_parent>0</wp:comment_parent>
<wp:commentmeta>
<wp:meta_key>_comment_rating</wp:meta_key>
<wp:meta_value><![CDATA[5]]></wp:meta_value>
</wp:commentmeta>
<comment:custom_data>
<comment:sentiment>positive</comment:sentiment>
<comment:verified>true</comment:verified>
</comment:custom_data>
</wp:comment>
</item>

<!-- Test 8: Multi-site namespace extensions -->
<item xmlns:multisite="http://wordpress.org/multisite/1.0/">
<title>Multi-site Data</title>
<wp:post_id>6</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Multi-site specific content]]></content:encoded>
<multisite:blog_id>3</multisite:blog_id>
<multisite:network_id>1</multisite:network_id>
<multisite:site_path>/subsite/</multisite:site_path>
</item>

<!-- Test 9: Namespace inheritance test -->
<item>
<title>Namespace Inheritance</title>
<wp:post_id>7</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Testing namespace inheritance]]></content:encoded>
<wp:postmeta xmlns:inherit="http://example.com/inherit/">
<wp:meta_key>_parent_data</wp:meta_key>
<wp:meta_value><![CDATA[parent_value]]></wp:meta_value>
<inherit:child_element>
<inherit:nested_data>This inherits the namespace</inherit:nested_data>
</inherit:child_element>
</wp:postmeta>
</item>

<!-- Test 10: Conditional namespace usage -->
<item xmlns:condition="http://example.com/conditional/">
<title>Conditional Namespaces</title>
<wp:post_id>8</wp:post_id>
<wp:post_type>post</wp:post_type>
<wp:status>publish</wp:status>
<content:encoded><![CDATA[Conditional namespace usage]]></content:encoded>
<condition:if test="feature_enabled">
<condition:feature_data>
<condition:enabled>true</condition:enabled>
<condition:config>advanced</condition:config>
</condition:feature_data>
</condition:if>
</item>

</channel>
</rss>
